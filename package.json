    "test": "jest"
  },
  "keywords": [],
  "author": "Guestline",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/md5": "2.3.1",
    "@types/mkdirp": "1.0.2",
    "@types/nock": "11.1.0",
    "@types/node": "17.0.3",
    "@types/prettier": "2.4.3",
    "@types/yargs": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "cross-env": "7.0.3",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.7",
    "md5": "^2.3.0",
    "nock": "13.2.2",
    "node-dev": "7.1.0",
    "prettier": "2.5.1",
