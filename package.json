{
  "name": "@guestlinelabs/onekey",
  "version": "0.0.9",
  "description": "Utility to download translations from the [OneSky](https://www.oneskyapp.com/) and generate typed keys in Typescript for having typed translations.",
  "bin": {
    "onekey": "lib/cli.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint src/**",
    "start": "cross-env TS_NODE_FILES=true node-dev -r ts-node/register src/cli.ts",
    "test": "jest"
  },
  "keywords": [],
  "author": "Guestline",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "14.14.14",
    "@types/prettier": "^2.1.5",
    "@types/yargs": "^15.0.12",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "cross-env": "7.0.3",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "3.3.0",
    "jest": "^26.6.3",
    "node-dev": "6.2.0",
    "prettier": "^2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@brainly/onesky-utils": "^1.4.1",
    "fp-ts": "^2.9.1",
    "fp-ts-contrib": "^0.1.21",
    "io-ts": "^2.2.13",
    "mkdirp": "^1.0.4",
    "yargs": "^16.2.0"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guestlinelabs/onekey.git"
  },
  "bugs": {
    "url": "https://github.com/guestlinelabs/onekey/issues"
  },
  "homepage": "https://github.com/guestlinelabs/onekey#readme"
