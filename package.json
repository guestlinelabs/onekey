    "@types/yargs": "17.0.33",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "cross-env": "7.0.3",
    "eslint": "8.7.0",
    "jest": "29.7.0",
    "md5": "^2.3.0",
    "nock": "13.5.6",
    "node-dev": "8.0.0",
